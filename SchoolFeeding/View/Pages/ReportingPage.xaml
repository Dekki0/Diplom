<UserControl x:Class="SchoolFeeding.View.Pages.ReportingPage"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SchoolFeeding.View.Pages"
             xmlns:viewModel="clr-namespace:SchoolFeeding.ViewModel"
              xmlns:utilities="clr-namespace:SchoolFeeding.Model.Utilities"
             mc:Ignorable="d" Height="193" Width="370">
    <StackPanel 
        Orientation="Vertical">

        


        <StackPanel Margin="10"
            HorizontalAlignment="Center"
            Orientation="Horizontal"/>

        <StackPanel Margin="10">
            <Button Margin="10"
                Content="Создать отчёт за 1 месяц" Height="34"
                    Command="{Binding ReportingOneCommand}"/>
            <Button Margin="10"
                Content="Создать отчёт за 3 месяц" Height="34"
                    Command="{Binding ReportingThreeCommand}"/>
        </StackPanel>
        
    </StackPanel>
</UserControl>
