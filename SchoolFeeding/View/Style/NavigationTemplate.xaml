<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel ="clr-namespace:SchoolFeeding.ViewModel"
                    xmlns:viewWindow ="clr-namespace:SchoolFeeding.View"
                    xmlns:viewPage="clr-namespace:SchoolFeeding.View.Pages">
    <DataTemplate DataType="{x:Type viewModel:LoginViewModel}">
        <viewWindow:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:MainViewModel}">
        <viewWindow:MainWindow/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:MenuViewModel}">
        <viewPage:MenuPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:InfoViewModel}">
        <viewPage:UsersInfoPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:AdminViewModel}">
        <viewPage:AdministratingPage/>
    </DataTemplate>
</ResourceDictionary>